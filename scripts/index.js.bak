    //Mobile
    function Mobile(){
        if($(window).width() < 720){
            //Intro Brand
            var betterWD = $('.introBrand .infoFirst div.better').outerWidth();
            var stepWD = $('.introBrand .infoFirst div.step').outerWidth();
            $('.introBrand .infoFirst').css({'width' : betterWD + stepWD});
            //Service
            $('.service').removeClass('on');
        }
    }
    Mobile();
    $(window).resize(function(){
        Mobile();
    })


});